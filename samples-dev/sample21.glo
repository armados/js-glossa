! KEYBOARD_INPUT: 'Π'
! KEYBOARD_INPUT: 'Τ'
! KEYBOARD_INPUT: 'Τ'
! KEYBOARD_INPUT: '3'
! KEYBOARD_INPUT: '3'
! KEYBOARD_INPUT: '1'
! KEYBOARD_INPUT: '1'
! KEYBOARD_INPUT: 'Τ'
! KEYBOARD_INPUT: '2'
! KEYBOARD_INPUT: '1'
! KEYBOARD_INPUT: '3'
! KEYBOARD_INPUT: 'Π'
! KEYBOARD_INPUT: 'Τ'
! KEYBOARD_INPUT: '1'
! KEYBOARD_INPUT: 'Τ'
! KEYBOARD_INPUT: '3'
! KEYBOARD_INPUT: '2'
! KEYBOARD_INPUT: 'Τ'
! KEYBOARD_INPUT: '1'
! KEYBOARD_INPUT: 'Π'
! KEYBOARD_INPUT: '2'
! KEYBOARD_INPUT: '3'
! KEYBOARD_INPUT: '2'
! KEYBOARD_INPUT: 'Π'
! KEYBOARD_INPUT: '1'
! KEYBOARD_INPUT: '1'
! KEYBOARD_INPUT: '2'
! KEYBOARD_INPUT: '2'
! KEYBOARD_INPUT: 'Τ'
! KEYBOARD_INPUT: '2'
! KEYBOARD_INPUT: 'Δ'
ΠΡΟΓΡΑΜΜΑ σουπερ
ΣΤΑΘΕΡΕΣ
  Ν = 5
ΜΕΤΑΒΛΗΤΕΣ
  ΑΚΕΡΑΙΕΣ: πλ, rear, front, ΟΥΡΑ[Ν], υπαλ, max, θεσημαξ, πλ1, πλ2, πλ3, υπαλπρ, πλσυνεχ
  ΧΑΡΑΚΤΗΡΕΣ: επ
  ΛΟΓΙΚΕΣ: συνεχ
 
 
ΑΡΧΗ
  front <- 0
  rear <- 0
 
  πλ <- 100
  πλ1 <- 0
  πλ2 <- 0
  πλ3 <- 0
  πλσυνεχ <- 0
  υπαλπρ <- 0
  συνεχ <- ΨΕΥΔΗΣ
 
  ΑΡΧΗ_ΕΠΑΝΑΛΗΨΗΣ
 
    ΑΡΧΗ_ΕΠΑΝΑΛΗΨΗΣ
      ΓΡΑΨΕ ' Π. ΠΕΛΑΤΗΣ'
      ΓΡΑΨΕ ' Τ. ΤΑΜΙΑΣ'
      ΓΡΑΨΕ ' Δ. ΔΙΕΥΘΥΝΤΗΣ'
      ΓΡΑΨΕ ' ΔΩΣΕ ΕΠΙΛΟΓΗ'
      ΔΙΑΒΑΣΕ επ
    ΜΕΧΡΙΣ_ΟΤΟΥ επ = 'Π' Η επ = 'Τ' Η επ = 'Δ'
 
 
    ΑΝ επ = 'Π' ΤΟΤΕ
 
      ΑΝ rear < Ν ΤΟΤΕ
        ΑΝ front = 0 ΚΑΙ rear = 0 ΤΟΤΕ
          front <- 1
        ΤΕΛΟΣ_ΑΝ
 
        πλ <- πλ + 1
 
        rear <- rear + 1
        ΟΥΡΑ[rear] <- πλ
        ΓΡΑΨΕ 'ο νεος πελατης ελαβε τον αριθμο ', πλ
      ΑΛΛΙΩΣ
        ΓΡΑΨΕ 'γεματη ουρα'
      ΤΕΛΟΣ_ΑΝ
 
    ΑΛΛΙΩΣ_ΑΝ επ = 'Τ' ΤΟΤΕ
 
      ΑΝ rear = 0 ΤΟΤΕ
        ΓΡΑΨΕ 'δεν υπαρχει ατομο στην ουρα'
      ΑΛΛΙΩΣ
        ΓΡΑΨΕ 'δωσε κωδικο ταμια'
        ΔΙΑΒΑΣΕ υπαλ
 
        ΓΡΑΨΕ 'ο ταμιας ', υπαλ, ' εξυπηρετει τΟν πελατη ', ΟΥΡΑ[front]
 
        ΑΝ front = rear ΤΟΤΕ
          front <- 0
          rear <- 0
        ΑΛΛΙΩΣ
          front <- front + 1
        ΤΕΛΟΣ_ΑΝ
 
        ΑΝ υπαλ = 1 ΤΟΤΕ
          πλ1 <- πλ1 + 1
        ΑΛΛΙΩΣ_ΑΝ υπαλ = 2 ΤΟΤΕ
          πλ2 <- πλ2 + 1
        ΑΛΛΙΩΣ
          πλ3 <- πλ3 + 1
        ΤΕΛΟΣ_ΑΝ
 
        ΑΝ υπαλ = υπαλπρ ΤΟΤΕ
          πλσυνεχ <- πλσυνεχ + 1
          ΑΝ πλσυνεχ = 3 ΤΟΤΕ
            συνεχ <- ΑΛΗΘΗΣ
          ΤΕΛΟΣ_ΑΝ
        ΑΛΛΙΩΣ
          πλσυνεχ <- 0
          υπαλπρ <- υπαλ
        ΤΕΛΟΣ_ΑΝ
 
 
      ΤΕΛΟΣ_ΑΝ
 
    ΤΕΛΟΣ_ΑΝ
 
  ΜΕΧΡΙΣ_ΟΤΟΥ επ = 'Δ'
 
  max <- πλ1
  θεσημαξ <- 1
  ΑΝ πλ2 > max ΤΟΤΕ
    max <- πλ2
    θεσημαξ <- 2
  ΤΕΛΟΣ_ΑΝ
  ΑΝ πλ3 > max ΤΟΤΕ
    max <- πλ3
    θεσημαξ <- 3
  ΤΕΛΟΣ_ΑΝ
  ΓΡΑΨΕ 'τους περισσοτερους πελατεσ εξυπηρετει το ταμειο ', θεσημαξ
 
  ΑΝ συνεχ = ΑΛΗΘΗΣ ΤΟΤΕ
    ΓΡΑΨΕ 'ΥΠΑΡΧΕΙ ΥΠΑΛΛΗΛΟΣ ΜΕ ΣΥΝΕΧΟΜΕΝΗ ΕΞΥΠΗΡΕΤΗΣΗ'
  ΤΕΛΟΣ_ΑΝ
 
 
 
ΤΕΛΟΣ_ΠΡΟΓΡΑΜΜΑΤΟΣ
 